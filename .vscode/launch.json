{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Real-Time Radar Processing",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/new_data_processing.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Jupyter Notebook",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "stopOnEntry": false,
            "program": "${file}",
            "cwd": "${workspaceFolder}"
        }
    ]
}