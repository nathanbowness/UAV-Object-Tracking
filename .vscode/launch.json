{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Real-Time Radar Processing",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/new_data_processing.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Object Tracking Program",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/object_tracking.py",
            "console": "integratedTerminal",
            "args": "--weights ${workspaceFolder}/yolo_video_object_tracking/yolov7.pt --source ${workspaceFolder}/yolo_video_object_tracking/video_samples/M0101.mp4 --conf-thres 0.4 --no-download --view-img"
        },
        {
            "name": "Current DEBUG",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/object_tracking.py",
            "console": "integratedTerminal",
            "args": "--weights yolo_video_object_tracking/yolov7.pt --source data/video/M0101.mp4 --conf-thres 0.4 --no-download --view-img --skip-video --radar-from-file --radar-source data/radar/run6-FD --enable-plot --skip-tracking"
        },
        {
        "name": "Doppler Analysis",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/radar_object_tracking/radarprocessing/micro_doppler_velocity_analysis.py",
        "console": "integratedTerminal",
        },
        {
            "name": "Yolo Object Detection - Video M0101",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/yolo_video_object_tracking/detect.py",
            "console": "integratedTerminal",
            "args": "--weights ${workspaceFolder}/yolo_video_object_tracking/yolov7.pt --source ${workspaceFolder}/yolo_video_object_tracking/video_samples/M0101.mp4 --conf-thres 0.4 --no-download --view-img"
        },
        {
            "name": "Yolo Object Detect and Track - Video M0101",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/yolo_video_object_tracking/detect.py",
            "console": "integratedTerminal",
            "args": "--weights ${workspaceFolder}/yolo_video_object_tracking/yolov7.pt --source ${workspaceFolder}/yolo_video_object_tracking/video_samples/M0101.mp4 --conf-thres 0.4 --no-download --view-img"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Jupyter Notebook",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "stopOnEntry": false,
            "program": "${file}",
            "cwd": "${workspaceFolder}"
        }
    ]
}